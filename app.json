{
  "expo": {
    "name": "DynamisApp",
    "slug": "DynamisApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Vector.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Vector.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "googleServicesFile": "./google-services.json",
        "foregroundImage": "./assets/Vector.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "androidNavigationBar": {
        "backgroundColor": "#000000",
        "barStyle": "light-content"
      },
      "package": "com.vn777.DynamisApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-localization"
    ]
  }
}